function ex512


x = linspace(-1, 2);
plot(x, f(x), 'r-')
xlabel("x")
ylabel("y")
xlim([-1 5]) 
ylim([0 1])

legend('Location','northeast')
legend("$g(\nu)$", "Interpreter","latex")
title("$Dual f_0(x)$ and $L(x,\lambda)$","Interpreter","latex")
axis tight;
hold off

saveas(gcf,"simple_optimization_problem","pdf")
function [y] = f(x)
    y = (-9 * (x.^2)) ./ (x + 1) + 1 + 8 .* x;
end


end